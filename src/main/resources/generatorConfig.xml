<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <properties resource="DbConfig.properties"></properties>
    <!--<classPathEntry location="${jdbc.driverLocation}"/>-->
    <classPathEntry location="/Users/Visupervi/.m2/repository/mysql/mysql-connector-java/8.0.21/mysql-connector-java-8.0.21.jar" />
    <context id="MysqlTable2" targetRuntime="Mybatis3">

        <commentGenerator>
            <!-- 去除自动生成的注释 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <!-- 数据库连接配置 -->
        <jdbcConnection driverClass="${driver}"
                        connectionURL="${url}"
                        userId="${uName}"
                        password="${uPwd}">
            <property name="nullCatalogMeansCurrent" value="true"></property>
        </jdbcConnection>
        <!--jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/test"
                        userId="root"
                        password="mysql" /-->

        <!-- 非必需，类型处理器，在数据库类型和java类型之间的转换控制-->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!--配置生成的实体包
            targetPackage：生成的实体包位置，默认存放在src目录下
            targetProject：目标工程名
         -->
        <javaModelGenerator targetPackage="com.HopeRun.SSM.Crm.entity"
                            targetProject="src/main/java"/>

        <!-- 实体包对应映射文件位置及名称，默认存放在src目录下 -->
        <sqlMapGenerator targetPackage="com.HopeRun.SSM.Crm.dao.mappers" targetProject="src/main/java"/>
        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.HopeRun.SSM.Crm.dao.mappers"
                             targetProject="src/main/java">
            <!-- enableSubPackages:是否让schema作为包的后缀 -->
            <property name="enableSubPackages" value="false" />
        </javaClientGenerator>

        <!-- 配置表
            schema：不用填写
            tableName: 表名
            enableCountByExample、enableSelectByExample、enableDeleteByExample、enableUpdateByExample、selectByExampleQueryId：
            去除自动生成的例子
        -->
        <table schema="crm" tableName="tbl_trackPoint" enableCountByExample="false" enableSelectByExample="false"
               enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false">
        </table>
<!--        <table schema="crm" tableName="sys_user" enableCountByExample="false" enableSelectByExample="false"-->
<!--               enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false">-->
<!--        </table>-->
<!--        <table schema="crm" tableName="customer" enableCountByExample="false" enableSelectByExample="false"-->
<!--               enableDeleteByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false">-->
<!--        </table>-->

    </context>
</generatorConfiguration>
